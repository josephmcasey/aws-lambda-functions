{
  "name": "typescript-aws-lambda",
  "version": "0.1.0",
  "description": "A package that manages and deploys AWS Lambda functions written in TypeScript.",
  "author": "me@josephmcasey.com",
  "keywords": [
    "aws",
    "lambda",
    "typescript",
    "ts",
    "webpack",
    "babel"
  ],
  "homepage": "https://github.com/josephmcasey/aws-lambda_codebuild-artifact-to-s3-website",
  "private": true,
  "scripts": {
    "clean": "npm run lint && rm -rf dist-test dist",
    "start": "npm run clean && tsc -p tsconfig-src.json && webpack --watch && rm -rf dist/src",
    "build": "npm run clean && tsc -p tsconfig-src.json && webpack --mode production && rm -rf dist/src",
    "package": "sam package --template-file sam.yaml --s3-bucket ll-aws-sam-artifacts --output-template-file sam.prod.yaml",
    "deploy": "aws cloudformation deploy --template-file ./sam.prod.yaml --capabilities CAPABILITY_NAMED_IAM --tags 'cli=true' 'automation=true' 'name=Lambda Functions' --stack-name sam-stack-for-aws-lambda",
    "lint": "tslint --fix --format codeFrame --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "pretest": "npm run clean && tsc -p tsconfig-test.json && BABEL_ENV=test babel dist-test --out-dir dist-test --source-maps",
    "test": "nyc ava 'dist-test/test/unit/**/*.test.js'"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-external-helpers": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@types/aws-lambda": "^8.10.6",
    "@types/jszip": "^3.1.4",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^10.3.0",
    "@types/proxyquire": "^1.3.28",
    "@types/request": "^2.47.0",
    "ava": "^0.25.0",
    "babel-loader": "^8.0.0-beta.3",
    "builtin-modules": "^3.0.0",
    "chalk": "^2.4.0",
    "nyc": "^11.7.1",
    "proxyquire": "^2.0.1",
    "require-self-ref": "^2.0.1",
    "source-map-support": "^0.5.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "aws-sdk": "^2.245.1",
    "bunyan": "^1.8.12",
    "jszip": "3.1.5",
    "mime-types": "2.1.18",
    "request": "^2.87.0"
  },
  "ava": {
    "require": [
      "require-self-ref",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
